{
    "additional-repositories": [
        "multilib"
    ],
    "archinstall-language": "English",
    "audio_config": {
        "audio": "pipewire"
    },
    "bootloader": "Grub",
    "config_version": "2.7.1",
    "debug": false,
    "disk_config": {
        "config_type": "pre_mounted_config",
        "mountpoint": "/mnt/archinstall"
    },
    "hostname": "archlinux",
    "kernels": [
        "linux-lts"
    ],
    "locale_config": {
        "kb_layout": "us",
        "sys_enc": "UTF-8",
        "sys_lang": "en_US"
    },
    "mirror_config": {
        "custom_mirrors": [],
        "mirror_regions": {
            "India": [
                "https://mirrors.nxtgen.com/archlinux-mirror/$repo/os/$arch",
                "https://mirror.sahil.world/archlinux/$repo/os/$arch",
                "https://in.mirrors.cicku.me/archlinux/$repo/os/$arch",
                "https://in-mirror.garudalinux.org/archlinux/$repo/os/$arch",
                "http://mirrors.nxtgen.com/archlinux-mirror/$repo/os/$arch",
                "http://mirror.sahil.world/archlinux/$repo/os/$arch",
                "http://mirror.albony.in/archlinux/$repo/os/$arch",
                "http://in.mirrors.cicku.me/archlinux/$repo/os/$arch",
                "http://in-mirror.garudalinux.org/archlinux/$repo/os/$arch",
                "http://archlinux.mirror.net.in/archlinux/$repo/os/$arch"
            ]
        }
    },
    "network_config": {
        "type": "nm"
    },
    "no_pkg_lookups": false,
    "ntp": true,
    "offline": false,
    "packages": [
        "neofetch"
    ],
    "parallel downloads": 0,
    "profile_config": {
        "gfx_driver": "All open-source",
        "greeter": "sddm",
        "profile": {
            "custom_settings": {
                "Hyprland": {
                    "seat_access": "polkit"
                }
            },
            "details": [
                "Hyprland"
            ],
            "main": "Desktop"
        }
    },
    "script": "guided",
    "silent": false,
    "skip_ntp": false,
    "skip_version_check": false,
    "swap": true,
    "timezone": "Asia/Kolkata",
    "uki": false,
    "version": "2.7.1"
}
